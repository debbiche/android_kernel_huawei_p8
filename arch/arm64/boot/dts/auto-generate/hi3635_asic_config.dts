		spi0: spi@fdf07000 {
			status = "ok";
			num-cs = <4>;
			pinctrl-names = "default", "idle";
			pinctrl-0 = <&gpio063_pmx_func &gpio065_pmx_func &gpio066_pmx_func &gpio067_pmx_func
					 &gpio068_pmx_func &gpio069_pmx_func &gpio063_cfg_func &gpio064_cfg_func
					 &gpio065_cfg_func &gpio066_cfg_func &gpio067_cfg_func &gpio068_cfg_func
					 &gpio069_cfg_func>;
			pinctrl-1 = <&gpio063_pmx_idle &gpio065_pmx_idle &gpio066_pmx_idle &gpio067_pmx_idle
					 &gpio068_pmx_idle &gpio069_pmx_idle &gpio063_cfg_idle &gpio064_cfg_idle
					 &gpio065_cfg_idle &gpio066_cfg_idle &gpio067_cfg_idle &gpio068_cfg_idle
					 &gpio069_cfg_idle>;
			#address-cells = <1>;
			#size-cells = <0>;
			//cs-gpios = <&gpio9 7 0>, <&gpio10 0 0>, <&gpio10 1 0>, <&gpio10 2 0>;

			/*spi_dev0: spi_dev0@0 {
				compatible = "spi_dev0";
				fpga_flag = <0>;
				spi-max-frequency = <100000>;
				reg = <0>;
				pl022,com-mode = <0>;
				pl022,interface = <0>;
				pl022,rx-level-trig = <3>;
				pl022,tx-level-trig = <3>;
				pl022,ctrl-len = <7>;
				pl022,wait-state = <0>;
				pl022,duplex = <0>;
				pl022,slave-tx-disable = <0>;
			};*/

			mini_isp: mini_isp@0 {
				compatible = "mini_isp";
				spi-max-frequency = <26000000>;
				reg = <0>;
				pl022,com-mode = <2>;
				pl022,interface = <0>;
				pl022,rx-level-trig = <3>;
				pl022,tx-level-trig = <3>;
				pl022,ctrl-len = <7>;
				pl022,wait-state = <0>;
				pl022,duplex = <0>;
				pl022,slave-tx-disable = <0>;

				mini_isp,reset_gpio = <23>;				
				mini_isp,cs_gpio = <66>;
				mini_isp,irq_gpio = <201>;
				mini_isp,dvdd_gpio = <96>;
				//mini_isp,vcc_gpio = <96>;
				iopw-supply = <&lsw52>;				
				pinctrl-names = "default", "idle";
				pinctrl-0 = <&gpio023_pmx_func &gpio023_cfg_func>;
				pinctrl-1 = <&gpio023_pmx_idle &gpio023_cfg_idle>;
			};

			spi_dev1: spi_dev1@1 {
				compatible = "spi_dev1";
				spi-max-frequency = <100000>;
				reg = <1>;
				pl022,com-mode = <0>;
				pl022,interface = <0>;
				pl022,rx-level-trig = <3>;
				pl022,tx-level-trig = <3>;
				pl022,ctrl-len = <7>;
				pl022,wait-state = <0>;
				pl022,duplex = <0>;
				pl022,slave-tx-disable = <0>;
			};

			spi_dev2: spi_dev2@2 {
				compatible = "spi_dev2";
				spi-max-frequency = <100000>;
				reg = <2>;
				pl022,com-mode = <0>;
				pl022,interface = <0>;
				pl022,rx-level-trig = <3>;
				pl022,tx-level-trig = <3>;
				pl022,ctrl-len = <7>;
				pl022,wait-state = <0>;
				pl022,duplex = <0>;
				pl022,slave-tx-disable = <0>;
			};

			spi_dev3: spi_dev3@3 {
				compatible = "spi_dev3";
				spi-max-frequency = <100000>;
				reg = <3>;
				pl022,com-mode = <0>;
				pl022,interface = <0>;
				pl022,rx-level-trig = <3>;
				pl022,tx-level-trig = <3>;
				pl022,ctrl-len = <7>;
				pl022,wait-state = <0>;
				pl022,duplex = <0>;
				pl022,slave-tx-disable = <0>;
			};
			fpga_spi: fpga_spi@4 {
				compatible = "fpga_spi";
				spi-max-frequency = <0x989680>;
				reg = <4>;
				pl022,com-mode = <2>;
				pl022,interface = <0>;
				pl022,rx-level-trig = <3>;
				pl022,tx-level-trig = <3>;
				pl022,ctrl-len = <7>;
				pl022,wait-state = <0>;
				pl022,duplex = <0>;
				pl022,slave-tx-disable = <0>;
			};
		};

		spi1: spi@fdf08000 {
			status = "ok";
			num-cs = <1>;
			pinctrl-names = "default", "idle";
			pinctrl-0 = <>;
			pinctrl-1 = <>;
			/*
			pinctrl-0 = <&gpio146_pmx_func &gpio147_pmx_func &gpio148_pmx_func &gpio149_pmx_func
				     &gpio146_cfg_func &gpio147_cfg_func &gpio148_cfg_func &gpio149_cfg_func>;
